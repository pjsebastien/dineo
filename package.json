{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:ssg": "npm run build && npm run preview & sleep 3 && node scripts/simple-prerender.mjs",
    "prerender": "node scripts/simple-prerender.mjs",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "reactSnap": {
    "source": "dist",
    "minifyHtml": {
      "collapseWhitespace": false,
      "removeComments": false
    },
    "puppeteerArgs": ["--no-sandbox", "--disable-setuid-sandbox"],
    "crawl": true,
    "include": [
      "/",
      "/activites-famille-reunion",
      "/activites-couple-reunion",
      "/activites-insolites-reunion",
      "/activites-pas-cheres-reunion",
      "/activites-a-offrir-reunion",
      "/que-faire-saint-pierre-reunion",
      "/que-faire-saint-leu-reunion",
      "/que-faire-cilaos-reunion",
      "/que-faire-piton-fournaise-reunion",
      "/que-faire-saint-benoit-reunion",
      "/mentions-legales",
      "/politique-confidentialite",
      "/cgu"
    ]
  },
  "dependencies": {
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^7.8.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitejs/plugin-react-swc": "^4.2.2",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "puppeteer": "^24.35.0",
    "react-snap": "^1.23.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2",
    "vite-plugin-ssg": "^0.1.0",
    "vite-plugin-static-copy": "^3.1.5",
    "vite-ssg": "^28.2.2"
  }
}
